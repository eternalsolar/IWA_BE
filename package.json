{
  "name": "IWATest",
  "version": "1.0.0",
  "description": "",
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "exec": "npm run serve"
  },
  "main": "dist/app.js",
  "scripts": {
    "compile": "tsc && node dist/app.js",
    "dev": "nodemon -e ts  --exec \"npm run compile\"",
    "start": "node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run tsc",
    "tsc": "tsc"
  },
  "author": "Esolar",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "crawler": "^1.2.2",
    "express": "^4.16.4",
    "psl": "^1.8.0",
    "redis": "^3.0.2",
    "youtube-thumbnail": "^1.3.0"
  }
}
